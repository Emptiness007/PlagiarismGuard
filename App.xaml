<Application x:Class="PlagiarismGuard.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PlagiarismGuard"
             StartupUri="Windows/SplashScreen.xaml"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="AnonymousProFont">/Fonts/#Anonymous Pro</FontFamily>
            <QuadraticEase x:Key="EaseOut" EasingMode="EaseOut"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
